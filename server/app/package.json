{
  "name": "missiv-server-app",
  "version": "0.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.mjs"
      }
    },
    "./drizzle.config.json": "./drizzle.config.json"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "drizzle-orm": "^0.30.6",
    "hono": "^4.1.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "drizzle-kit": "^0.20.14",
    "pkgroll": "^2.0.2",
    "wrangler": "^3.32.0"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:up": "drizzle-kit up:sqlite"
  }
}
